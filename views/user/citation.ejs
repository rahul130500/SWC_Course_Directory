    <div class="font-bold text-2xl text-black ml-32 mb-8">Details for citation</div>
    <form class="mb-16" method="post" action="">
    <div class="flex flex-wrap ml-32">
    <div class="flex items-center mb-8">
    <label class="text-2xl text-black mr-7" for="name">Your Name</label>
    <p class="border-2 border-black text-black px-2 py-2" style="width:401px; height:60px"><%= user.username %></p></div>
    <div class="flex items-center mb-8 ml-20">
    <label class="text-2xl text-black mr-7" for="coursecode">Course Name/Code</label>
    <input class="border-2 border-black text-black px-2 py-2" style="width:401px; height:60px"  type="text" placeholder="Course Name" name="course_name"></div>
    </div><div class="flex flex-wrap ml-32">
    <div class="flex items-center mb-8 ">
    <label class="text-2xl text-black mr-7" for="email">Your e-mail</label>
    <p class="border-2 border-black text-black px-2 py-2" style="width:401px; height:60px"><%= user.outlookID %></p></div>
    <div class="flex items-center mb-8 ml-10">
    <label class="text-2xl text-black mr-7" for="professor">Professor/CD Members</label>
    <input class="border-2 border-black text-black px-2 py-2" style="width:401px; height:60px"  type="text" placeholder="professor" name="professor"></div>
    </div><div class="flex flex-wrap ml-32">
    <div class="flex ">
    <label class="text-2xl text-black mr-7" for="reason">Reason</label>
    <textarea style="width: 1139px; height: 180px;" class="border-2 border-black text-black px-2 py-2" type="text" placeholder="Reason" name="reason"></textarea></div>
    </div><div class="flex justify-end mx-36"><button class="bg-blue-600 px-16 py-4 text-xl rounded-md text-white mt-4" type="submit">Send</button></div></form>
