    <form class="mb-16" method="post" action="/coursedirectory/contribute">
        <div class="md:flex md:flex-wrap ml-2 md:ml-32 ">
            <div class="md:flex items-center mb-8 w-full md:w-1/2 ">
            <label class="text-2xl text-black w-1/3" for="name">Your Name</label>
            <p class="border-2 border-black text-black px-2 py-2 w-full md:w-1/2 " style="height:60px"><%= user.username %></p></div>
            <div class="md:flex items-center mb-8 w-full md:w-1/2 ">
                <label class="text-2xl text-black w-1/3" for="askto">Course Name</label>
                <input class="border-2 border-black text-black px-2 py-2 w-full md:w-1/2 " style="height:60px"  type="text" placeholder="Course Name" name="course_name"></div>
            <div class="md:flex items-center mb-8 w-full md:w-1/2 ">
            <label class="text-2xl text-black w-1/3" for="email">Your e-mail</label>
            <p class="border-2 border-black text-black px-2 py-2 w-full md:w-1/2 " style=" height:60px"><%= user.outlookID %></p></div>
            <div class="md:flex items-center mb-8 w-full md:w-1/2 ">
                <label class="text-2xl text-black w-1/3" for="topic">Topic</label>
                <input class="border-2 border-black text-black px-2 py-2 w-full md:w-1/2 " style="height:60px"  type="text" placeholder="Topic" name="topic"></div>
            <div class="md:flex w-full mb-8">
            <label class="text-2xl text-black w-1/6" for="query">Link</label>
            <input style=" height:60px" class="border-2 border-black text-black px-2 py-2 w-full md:w-4/6 " type="text" placeholder="Link" name="link"></div>
            <div class="md:flex w-full">
                <label class="text-2xl text-black w-1/6" for="query">Description</label>
                <textarea style=" height: 180px;" class="border-2 border-black text-black px-2 py-2 w-full md:w-4/6 " type="text" placeholder="description" name="description"></textarea></div>
            </div>
    <div class="flex justify-center "><button class="bg-blue-600 px-16 py-4 text-xl rounded-md text-white mt-4" type="submit">Send</button></div></form>
